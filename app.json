{
  "expo": {
    "name": "FitJourney Tracker",
    "slug": "fitjourney-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.fitjourney-tracker",
      "infoPlist": {
        "NSHealthShareUsageDescription": "We use your Health data to track steps and workouts.",
        "NSHealthUpdateUsageDescription": "We update your Health data with workout sessions.",
        "UIBackgroundModes": [
          "background-processing",
          "background-fetch",
          "remote-notification"
        ]
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "app.rork.fitjourneytracker"
    },
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "e8c3207a-2d9c-438a-a2b8-30a2fc1076da"
      }
    }
  }
}
